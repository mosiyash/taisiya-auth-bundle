{
  "name": "taisiya/auth-bundle",
  "license": "MIT",
  "type": "taisiya-bundle",
  "description": "The \"Auth Bundle\".",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Taisiya\\AuthBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Taisiya\\Tests\\": "tests/"
    }
  },
  "require": {
    "taisiya/core-bundle": "dev-master",
    "aura/auth": "^2.0"
  },
  "require-dev": {
    "composer/composer": "*"
  },
  "scripts": {
    "post-install-cmd": [
      "Taisiya\\AuthBundle\\Composer\\DevScriptHandler::copySettings",
      "Taisiya\\AuthBundle\\Composer\\DevScriptHandler::mergeSettings",
      "Taisiya\\AuthBundle\\Composer\\DevScriptHandler::createPhinxConfigFile"
    ],
    "post-update-cmd": [
      "Taisiya\\AuthBundle\\Composer\\DevScriptHandler::copySettings",
      "Taisiya\\AuthBundle\\Composer\\DevScriptHandler::mergeSettings",
      "Taisiya\\AuthBundle\\Composer\\DevScriptHandler::createPhinxConfigFile"
    ]
  }
}
